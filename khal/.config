[calendars]

[[personal]]
path = /home/nicholas/.local/share/khal/calendars/personal/
color = yellow

[[olds]]
path = /home/nicholas/.local/share/khal/calendars/olds/
color = light red

[[work]]
path = /home/nicholas/.local/share/khal/calendars/work/
color = light blue

[[eca]]
path = /home/nicholas/.local/share/khal/calendars/eca/
color = light green

# black, white, brown, yellow, dark gray, dark green, dark blue, light gray, light green
# , light
# blue, dark magenta, dark cyan, dark red, light magenta, light cyan, light red

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

# default entry format 2018-11-10 19:00 21:00 hihihih

[default]

default_command = calendar
highlight_event_days = True
timedelta = 7d
